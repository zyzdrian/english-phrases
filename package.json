{
  "name": "english-phraseals",
  "version": "0.9.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@testing-library/jest-dom": "4.2.3",
    "@testing-library/react": "9.3.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "2.8.0",
    "@typescript-eslint/parser": "2.8.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.0.6",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "fetch-mock": "^8.0.0",
    "husky": "^3.1.0",
    "jest-fetch-mock": "^2.1.2",
    "react-docgen-typescript-loader": "3.3.0",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "react-test-renderer": "^16.12.0",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "@reach/router": "1.2.1",
    "@storybook/addon-actions": "5.2.6",
    "@storybook/addon-knobs": "5.2.6",
    "@storybook/addon-links": "5.2.6",
    "@storybook/addons": "5.2.6",
    "@storybook/react": "5.2.6",
    "@types/jest": "24.0.22",
    "@types/node": "12.12.7",
    "@types/reach__router": "1.2.6",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/react-transition-group": "4.2.3",
    "@types/styled-components": "4.1.8",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-scripts": "3.2.0",
    "react-transition-group": "4.3.0",
    "serve": "11.2.0",
    "styled-components": "4.4.1",
    "typescript": "3.7.2",
    "workbox-build": "4.3.1"
  },
  "scripts": {
    "eslint": "eslint src/**/*.{tsx,ts}",
    "dev": "react-scripts start",
    "start": "serve -s build",
    "build": "react-scripts build && yarn run build-sw",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-sw": "node ./src/sw-build.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/serviceWorker.ts",
      "!<rootDir>/src/sw-build.js",
      "!<rootDir>/src/sw-template.js",
      "!<rootDir>/src/contexts/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
